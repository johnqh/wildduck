# Local Development Overrides (.env.local)
# This file is ignored by git and used for local development overrides
# Copy this to .env.local and customize for your local environment

# Override mail_box_indexer URL for local development
MAIL_BOX_INDEXER_URL=http://localhost:42069

# Enable rate limiting for local testing
ENABLE_RATE_LIMITING=true

# Local development settings
NODE_ENV=development
UV_THREADPOOL_SIZE=32

# Debug settings (development only)
DEBUG_TEST_QUERY=true
DEBUG_CONSOLE=false

# Local webhook for testing notifications
ZONE_WEBHOOK_URL=http://localhost:3000/webhook